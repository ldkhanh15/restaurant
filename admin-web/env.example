# Next.js Public Environment Variables (build-time)
# These are embedded at build time, so rebuild is needed if changed
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXT_PUBLIC_CLOUDINARY_API_KEY=
NEXT_PUBLIC_CLOUDINARY_API_SECRET=

# Next.js Runtime Environment
NODE_ENV=production

